				<Node id="INCUS ('ANVIL')" alignment="2" arrowcolor="-16738048" arrowsize="1" backgroundcolor="-1" foregroundcolor="-16777216" initialexpanded="0" linecolor="-13395712" linesize="1" fontsize="1" fontstyle="bold">
