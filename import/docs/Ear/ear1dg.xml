		<Node id="INNER EAR / LABYRINTH" transition="Has 2 components" transitionplacement="1" transitionfontsize="1" alignment="1" arrowcolor="-16737793" arrowsize="2" backgroundcolor="-1" foregroundcolor="-16777216" initialexpanded="0" linecolor="-16737793" linesize="2" fontsize="2" fontstyle="bold">
