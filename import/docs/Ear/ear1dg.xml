	<Node id="ANATOMY OF THE EAR" transition="Anatomically the ear is divided into 3 parts:" transitionplacement="1" transitionfontsize="1" alignment="1" arrowcolor="-3407617" arrowsize="2" backgroundcolor="-1" foregroundcolor="-16777216" initialexpanded="0" linecolor="-3394561" linesize="3" fontsize="3" fontstyle="bold,italic">
		<Node id="EXTERNAL EAR" transition="Consists of 2 parts:" transitionplacement="1" transitionfontsize="1" alignment="1" arrowcolor="-65536" arrowsize="2" backgroundcolor="-1" foregroundcolor="-16777216" initialexpanded="0" linecolor="-65536" linesize="2" fontsize="2" fontstyle="bold">
