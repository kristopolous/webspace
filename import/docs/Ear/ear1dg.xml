		<Node id="MIDDLE EAR / TYMPANIC CAVITY" alignment="1" arrowcolor="-16738048" arrowsize="2" backgroundcolor="-1" foregroundcolor="-16777216" initialexpanded="0" linecolor="-16738048" linesize="2" fontsize="2" fontstyle="bold">
			<Node id="Has 3 Functions" alignment="2" arrowcolor="-16738048" arrowsize="1" backgroundcolor="-1" foregroundcolor="-16777216" initialexpanded="0" linecolor="-16738048" linesize="1" fontsize="1">
				<Item id="Transmits vibration into the inner ear." alignment="2" arrowcolor="-65536" arrowsize="1" backgroundcolor="-1" foregroundcolor="-16777216" initialexpanded="0" linecolor="-16776961" linesize="1" fontsize="1"></Item>
				<Item id="Equalizes pressure between outside inside of ear to prevent rupture of      tympanic" alignment="2" arrowcolor="-65536" arrowsize="1" backgroundcolor="-1" foregroundcolor="-16777216" initialexpanded="0" linecolor="-16776961" linesize="1" fontsize="1"></Item>
				<Item id="Reduces amplitude or vibration of ossicles to prevent loud noise." alignment="2" arrowcolor="-65536" arrowsize="1" backgroundcolor="-1" foregroundcolor="-16777216" initialexpanded="0" linecolor="-16776961" linesize="1" fontsize="1"></Item>
			</Node>
			<Node id="Has 3 Communications" alignment="2" arrowcolor="-16738048" arrowsize="1" backgroundcolor="-1" foregroundcolor="-16777216" initialexpanded="0" linecolor="-13395712" linesize="1" fontsize="1">
